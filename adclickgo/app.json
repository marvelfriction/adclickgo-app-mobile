{
  "expo": {
    "name": "Adclickgo",
    "slug": "adclickgo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/adclickgo-3d-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#69C52F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marvelfriction.adclickgo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adclickgo-3d-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marvelfriction.adclickgo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/adclickgo-3d-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eab15b3a-6874-4e1e-98bd-d8b22aea1a37"
      }
    }
  }
}
